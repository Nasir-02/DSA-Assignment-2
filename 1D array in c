#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
 int num,i,sum=0;
    scanf("%d\n",&num);
    int arr[num];
    
    for(i=0;i<num;i++){    
    scanf("%d",&arr[i]) ; 
    }
    for(i=0;i<num;i++)
    { 
    sum+=arr[i];
    }
    printf("%d\n",sum);
    
    return 0;
}
